{
  "name": "kumu",
  "version": "0.0.1",
  "license": "MIT",
  "main": "server/server.js",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run nodemon\"",
    "nodemon": "nodemon",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings install",
    "postinstall": "typings install && npm run build",
    "lb-ng": "lb-ng ./server/server.js ./client/src/app/lb-services.ts -l angular2",
    "build": "gulp copylibs && npm run build:sui",
    "build:sui": "gulp --gulpfile ./client/src/semantic-ui/gulpfile.js build",
    "console": "loopback-console .",
    "webserver:dev": "lite-server"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "bcrypt": "^0.8.5",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "jquery": "^2.2.3",
    "loopback": "^2.22.0",
    "loopback-boot": "^2.18.1",
    "loopback-component-explorer": "^2.1.0",
    "loopback-connector-mongodb": "^1.13.3",
    "loopback-datasource-juggler": "^2.46.0",
    "nodemon": "^1.9.1",
    "pinyin": "^2.7.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "semantic-ui": "^2.1.8",
    "serve-favicon": "^2.0.1",
    "systemjs": "^0.19.26",
    "zone.js": "^0.6.11"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "^3.9.0",
    "gulp-watch": "^4.3.5",
    "gulp-webserver": "^0.9.1",
    "lite-server": "^2.2.0",
    "loopback-console": "^0.2.2",
    "loopback-sdk-angular": "github:qeti/loopback-sdk-angular#188-angular2-support",
    "loopback-sdk-angular-cli": "github:qeti/loopback-sdk-angular-cli#37-angular2-support",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lunchboxer/kumu.git"
  },
  "description": "kumu"
}

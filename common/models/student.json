{
  "name": "Student",
  "plural": "Students",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "EnglishName": {
      "type": "string",
      "required": true
    },
    "ChineseName": {
      "type": "string",
      "required": true
    },
    "PinyinName": {
      "type": "string",
      "required": false
    },
    "id": {
      "type": "number",
      "id": true,
      "required": true,
      "description": "Unique number from student ID card."
    },
    "gender": {
      "type": "string",
      "required": false,
      "description": "'M' for male, 'F' for female"
    },
    "birthdate": {
      "type": "Date",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "studentGroups": {
      "type": "hasAndBelongsToMany",
      "model": "StudentGroup",
      "foreignKey": ""
    },
    "classes": {
      "type": "hasMany",
      "model": "Class",
      "foreignKey": "",
      "through": "Seating"
    }
  },
  "acls": [],
  "methods": {}
}
